{
    "manifest_version": 2,
    "name": "KiteScrapy",
    "version": "0.6",

    "browser_action": {
        "default_icon": "scrapper.png",
        "default_popup": "popup.html",
 		"default_title": "Kite Scrapper"
      },

    "content_scripts": [
        {
            "matches":[
                "https://kite.zerodha.com/*"
            ],
            "js": [ "jquery-3.1.0.min.js", "socket.io.min.js", "content.js"]
        }
    ]

}